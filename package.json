    "jsdom": "^27.0.1",
    "shadcn": "^3.5.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^4.0.3",
    "wait-on": "^9.0.1"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
